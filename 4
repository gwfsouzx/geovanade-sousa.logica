class animal:
    def emitir_som(self):
        pass
class cachorro(animal):
     def emitir_som(self):
            print("au au")
class gato(animal):
     def emitir_som(self):
            print("miau")
            
            #teste
cachorro = cachorro()
gato = gato()
cachorro.emitir_som()
gato.emitir_som()
            