class Pessoa: 
    def __init__(self, nome, idade):
        self.nome = nome
        self.idade = idade
    def exibir_informacoes(self):
        print(f"nome:{self.nome}, idade:{self.idade}")
            
 #teste
p1 = Pessoa("jo√£o", 30)
p2 = Pessoa("maria", 25)
p1.exibir_informacoes()
p2.exibir_informacoes()